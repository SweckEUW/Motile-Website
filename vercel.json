{
    "version": 2,
    "builds": [
        {
            "src": "Backend/server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "Backend/server/public/**",
            "use": "@vercel/static"
        }

    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Backend/server/index.js"
        },
        {
            "src": "/(.+)",
            "dest": "Backend/server/public/$1"
        }
    ]
}