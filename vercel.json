{
    "version": 2,
    "builds": [
        {
            "src": "Backend/server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "Backend/server/public/**",
            "use": "@vercel/static"
        }

    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "Backend/server/index.js"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
    ]
}